<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:CollectionViewTouchEffect"
             x:Class="CollectionViewTouchEffect.OtherPage"
             x:DataType="local:OtherPageModel">
    <Grid RowDefinitions="*,*">
        <Label Grid.Row="0" Text="{Binding RowText}" HorizontalOptions="Center" VerticalOptions="Center" />
        <Button Grid.Row="1" Text="Go Back" Command="{Binding GoBackCommand}" HorizontalOptions="Center" VerticalOptions="Center" />
    </Grid>
</ContentPage>